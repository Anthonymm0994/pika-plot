{
  "plot_type": "TimeSeries",
  "title": "Weather Monitoring Dashboard - Interactive Time Series",
  "x_label": "Time (Hours)",
  "y_label": "Measurements",
  "width": 1200,
  "height": 800,
  "specific": {
    "type": "TimeSeriesConfig",
    "time_column": "timestamp",
    "value_columns": ["temperature", "humidity", "pressure"],
    "aggregation": "None",
    "show_range_selector": true
  },
  "interactive_features": {
    "zoom_enabled": true,
    "pan_enabled": true,
    "legend_visible": true,
    "legend_position": "bottom",
    "grid_visible": true,
    "grid_style": "dashed",
    "axis_labels_visible": true,
    "axis_labels_font_size": 12,
    "title_font_size": 18,
    "tooltip_enabled": true,
    "tooltip_format": "Time: {x}, {series}: {y:.2f}",
    "crosshair_enabled": true,
    "line_highlighting": true,
    "range_selector": true,
    "time_brush": true
  },
  "styling": {
    "background_color": "#ffffff",
    "grid_color": "#e8e8e8",
    "axis_color": "#666666",
    "text_color": "#333333",
    "line_colors": ["#FF6B6B", "#4ECDC4", "#45B7D1"],
    "line_widths": [2.5, 2.5, 2.5],
    "point_sizes": [4, 4, 4]
  },
  "range_selector": {
    "buttons": [
      {"label": "1H", "value": "1h"},
      {"label": "6H", "value": "6h"},
      {"label": "12H", "value": "12h"},
      {"label": "1D", "value": "1d"},
      {"label": "All", "value": "all"}
    ],
    "height": 60
  }
} 