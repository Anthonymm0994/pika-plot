{
  "plot_type": "Scatter",
  "title": "Sales Performance Analysis - Interactive Scatter Plot",
  "x_label": "Sales Amount ($)",
  "y_label": "Quantity Sold (Units)",
  "width": 1000,
  "height": 700,
  "specific": {
    "type": "ScatterConfig",
    "x_column": "sales",
    "y_column": "quantity",
    "size_column": "price",
    "color_column": "category",
    "point_radius": 6.0,
    "marker_shape": "Circle"
  },
  "interactive_features": {
    "zoom_enabled": true,
    "pan_enabled": true,
    "legend_visible": true,
    "legend_position": "top_right",
    "grid_visible": true,
    "grid_style": "dotted",
    "axis_labels_visible": true,
    "axis_labels_font_size": 14,
    "title_font_size": 18,
    "tooltip_enabled": true,
    "tooltip_format": "Sales: ${x:.2f}, Quantity: {y} units, Category: {color}",
    "crosshair_enabled": true,
    "selection_enabled": true,
    "brush_selection": true
  },
  "styling": {
    "background_color": "#ffffff",
    "grid_color": "#e0e0e0",
    "axis_color": "#333333",
    "text_color": "#333333",
    "color_palette": ["#1f77b4", "#ff7f0e", "#2ca02c", "#d62728", "#9467bd"]
  }
} 